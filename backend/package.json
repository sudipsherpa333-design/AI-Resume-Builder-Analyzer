{
  "name": "ai-resume-builder-backend",
  "version": "1.0.0",
  "description": "Backend for AI Resume Builder with Admin System",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:watch": "nodemon --watch src --watch server.js --ext js,json server.js",
    "prod": "NODE_ENV=production node server.js",
    "prod:cluster": "NODE_ENV=production ENABLE_CLUSTER=true node server.js",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf node_modules package-lock.json",
    "seed": "node src/scripts/seedAdmin.js",
    "debug": "node --inspect server.js",
    "lint": "eslint .",
    "fix": "eslint . --fix"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "connect-mongo": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.21.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^6.15.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^2.4.5",
    "rate-limit-redis": "^4.3.1",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.11.1",
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "resume",
    "builder",
    "backend",
    "express"
  ],
  "author": "Sudipro",
  "license": "MIT"
}
