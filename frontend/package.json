{
  "name": "ai-resume-builder-frontend",
  "version": "1.2.0",
  "type": "module",
  "private": true,
  "description": "AI-powered Resume Builder with modern React stack",
  "scripts": {
    "dev": "vite --mode development",
    "dev:hot": "vite --mode development --host 0.0.0.0 --port 3000",
    "dev:analyze": "vite --mode analyze",
    "dev:network": "vite --mode development --host 0.0.0.0",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --port 4173 --host",
    "preview:network": "vite preview --port 4173 --host 0.0.0.0",
    "start": "vite --host 0.0.0.0 --port 3000",
    "dev:both": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgGreen.bold\" -p \"[{name}]\" \"cd ../backend && npm run dev\" \"npm run dev\"",
    "dev:full": "concurrently -n \"BACKEND,FRONTEND\" -c \"blue.bold,green.bold\" \"cd ../backend && npm run dev\" \"vite --mode development --host 0.0.0.0 --port 3000\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:strict": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\" \"public/**/*.html\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\" \"public/**/*.html\"",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "check": "npm run lint && npm run format:check",
    "check:all": "npm run lint && npm run format:check && npm run type-check",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "clean": "rm -rf node_modules dist .turbo .vite coverage .next",
    "clean:dist": "rm -rf dist",
    "clean:cache": "rm -rf .vite node_modules/.vite",
    "reinstall": "npm run clean && npm install",
    "analyze": "vite-bundle-analyzer",
    "install-all": "npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstall": "echo '✅ Dependencies installed successfully!' && npm run type-check",
    "health": "curl -f http://localhost:3000/api/health || echo '❌ Backend not reachable'",
    "reset": "rm -rf node_modules package-lock.json && npm install",
    "kill-ports": "npx kill-port 3000 5001 4173 5173 5174 5175 8080 3001",
    "start-clean": "npm run kill-ports && npm run dev",
    "dev-safe": "npm run kill-ports && npm run dev",
    "serve": "serve dist -p 3000",
    "serve:network": "serve dist -p 3000 -l",
    "build:serve": "npm run build && npm run serve",
    "docker:build": "docker build -t ai-resume-builder-frontend .",
    "docker:run": "docker run -p 3000:3000 ai-resume-builder-frontend",
    "update": "npm update --save",
    "update:all": "npm update --save --save-dev",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:fix-force": "npm audit fix --force",
    "outdated": "npm outdated",
    "depcheck": "npx depcheck",
    "size": "vite-bundle-analyzer dist/stats.html",
    "generate-icons": "node scripts/generate-icons.js",
    "generate-favicon": "node scripts/generate-favicon.js",
    "fix-deps": "npm install --legacy-peer-deps && rm -rf node_modules/.vite",
    "ci-install": "npm ci --legacy-peer-deps",
    "verify-deps": "npm ls class-variance-authority tailwind-merge"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "^16.3.0",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@react-oauth/google": "^0.12.1",
    "@sentry/react": "^10.34.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.90.19",
    "antd": "^5.19.0",
    "axios": "^1.7.2",
    "axios-retry": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "framer-motion": "^10.18.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lucide-react": "^0.563.0",
    "mongoose": "^9.1.1",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^4.12.0",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.30.3",
    "recharts": "^2.15.4",
    "serve": "^14.2.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@playwright/test": "^1.40.1",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-bundle-analyzer": "^1.3.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11",
      "not ie_mob 11",
      "not samsung 4"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "keywords": [
    "resume-builder",
    "ai",
    "react",
    "vite",
    "dashboard",
    "resume",
    "cv",
    "career",
    "job-search",
    "ats",
    "resume-analyzer"
  ],
  "author": "AI Resume Builder Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-resume-builder.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-resume-builder/issues"
  },
  "homepage": "https://github.com/yourusername/ai-resume-builder#readme",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/yourusername"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "overrides": {
    "class-variance-authority": "^0.7.1",
    "tailwind-merge": "^3.4.0"
  }
}
