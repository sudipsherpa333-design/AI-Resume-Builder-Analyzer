{
  "name": "ai-resume-builder-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "AI-powered Resume Builder with modern React stack",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite --host",
    "dev:both": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgGreen.bold\" \"cd ../backend && npm run dev\" \"npm run dev\"",
    "dev:backend": "cd ../backend && npm run dev",
    "dev:frontend": "vite",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run format:check",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "clean": "rm -rf node_modules dist .turbo .vite",
    "clean:dist": "rm -rf dist",
    "reinstall": "npm run clean && npm install",
    "analyze": "vite-bundle-analyzer",
    "install-all": "npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstall": "echo 'âœ… Dependencies installed successfully!'",
    "health": "curl http://localhost:5174/api/health || echo 'Backend not reachable'",
    "fix-babel": "npm install @babel/plugin-transform-runtime @babel/runtime @babel/core @babel/preset-env @babel/preset-react --save-dev",
    "reset": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "^16.3.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@react-oauth/google": "^0.12.1",
    "@tanstack/react-query": "^5.52.2",
    "antd": "^5.19.0",
    "axios": "^1.7.2",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "lucide-react": "^0.309.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-bundle-analyzer": "^1.3.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "resume-builder",
    "ai",
    "react",
    "vite",
    "dashboard",
    "resume",
    "cv",
    "career",
    "job-search"
  ],
  "author": "AI Resume Builder Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-resume-builder.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-resume-builder/issues"
  },
  "homepage": "https://github.com/yourusername/ai-resume-builder#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
